{
  "printWidth": 100,
  "importOrder": [
    "<TYPES>",
    "<TYPES>^[.]",
    "",
    "^(solid-js/(.*)|solid-js)$",
    "^@tanstack/(.*)$",
    "^@tauri/(.*)$",
    "<BUILTIN_MODULES>",
    "<THIRD_PARTY_MODULES>",
    "",
    ".astro$",
    "^~/(.*)$",
    "^[.]",
    "",
    ".css$"
  ],
  "svelteAllowShorthand": false,
  "tailwindStylesheet": "./src/styles.css",
  "tailwindFunctions": [
    "cx"
  ],
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ]
}