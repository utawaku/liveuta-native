{
  "printWidth": 100,
  "importOrder": [
    "<BUILTIN_MODULES>",
    "<TYPES>",
    "<TYPES>^[.]",
    "<THIRD_PARTY_MODULES>",
    "^~/(.*)$",
    "^[.]",
    ".css$"
  ],
  "svelteAllowShorthand": false,
  "tailwindStylesheet": "./src/app.css",
  "tailwindFunctions": [
    "cx"
  ],
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ]
}